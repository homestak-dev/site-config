# secrets.yaml — Sensitive values for your homestak deployment
#
# Copy this file to secrets.yaml:
#   cp secrets.yaml.example secrets.yaml
#
# Most values are auto-populated by 'homestak pve-setup'.
# See docs for optional SOPS/age encryption after secrets exist.

# API tokens — auto-generated by 'homestak pve-setup'
api_tokens: {}

# VM root password hash (generate with: openssl passwd -6)
passwords:
  vm_root: ""

# SSH public keys — auto-populated by 'homestak site-init'
ssh_keys: {}

# Signing key for provisioning tokens (auto-generated if empty)
auth:
  signing_key: ""
